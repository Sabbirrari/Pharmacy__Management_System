<AnchorPane id="AnchorPane" prefHeight="454.0" prefWidth="923.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="address_book_management.DashboardController">
   <children>
      <Label alignment="CENTER" layoutY="-1.0" prefHeight="95.0" prefWidth="923.0" style="-fx-background-color: #E5E4E2;" text="Welcome Address Book Management System">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="c_name" layoutX="22.0" layoutY="111.0" prefHeight="34.0" prefWidth="198.0" promptText="Client Name" />
      <TextField fx:id="c_address" layoutX="22.0" layoutY="156.0" prefHeight="34.0" prefWidth="198.0" promptText="Client Address" />
      <TextField fx:id="c_phone" layoutX="22.0" layoutY="200.0" prefHeight="34.0" prefWidth="198.0" promptText="Client Phone Number" />
      <TextField fx:id="c_email" layoutX="22.0" layoutY="243.0" prefHeight="34.0" prefWidth="198.0" promptText="Client Email" />
      <TextField fx:id="c_gender" layoutX="22.0" layoutY="288.0" prefHeight="34.0" prefWidth="198.0" promptText="Gender" />
      <Button fx:id="btn5" layoutX="22.0" layoutY="336.0" mnemonicParsing="false" onAction="#added" prefHeight="26.0" prefWidth="58.0" text="Add" />
      <Button fx:id="btn6" layoutX="97.0" layoutY="336.0" mnemonicParsing="false" onAction="#delete" prefHeight="26.0" prefWidth="50.0" text="Delete" />
      <Button fx:id="btn7" layoutX="162.0" layoutY="336.0" mnemonicParsing="false" onAction="#update" prefHeight="26.0" prefWidth="57.0" text="Update" />
      <TableView fx:id="tableview" layoutX="244.0" layoutY="113.0" prefHeight="250.0" prefWidth="675.0" />
      <Button fx:id="btn8" layoutX="842.0" layoutY="402.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      <TextField fx:id="c_search" layoutX="23.0" layoutY="380.0" prefHeight="43.0" prefWidth="198.0" promptText="Search by name, phone, email..." />
   </children>
</AnchorPane>
